<% provide(:title, "Editing Event") %>
<div class="col-lg-12">
  <div class="card">
    <div class="card-header blue darken-3 white-text">
      <h3 class="card-title" style="text-align: center;">
        <i class="far fa-edit"></i>
      </h3>
    </div>
    <div class="card-body">
      <h2><%= @event.title %></h2>
      <%= render 'layouts/eventsparams', url: nil, submit_text: "Save Edits" %>
    </div>
  </div>
</div>
